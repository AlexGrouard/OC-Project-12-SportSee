{
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "source": {
        "include": ["src"],
        "includePattern": "\\.(jsx|js|ts|tsx)$"
        
    },
    "plugins": [
        "node_modules/better-docs/typescript"
    
    ],
    "typescript": {
        "moduleRoot": "test"
    },
     "templates": {
      "better-docs": {
          "name": "My React components"
        }
    },
    "opts": {
        "template": "node_modules/better-docs",
        "destination": "docs",
        "recurse": true,
        "readme": "README.md"
    }
}